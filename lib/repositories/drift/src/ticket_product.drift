CREATE TABLE IF NOT EXISTS "ticket_product" (
    "id" TEXT NOT NULL,
    "product_id" TEXT NOT NULL,
    "ticket_id" TEXT NOT NULL,
    "quantity" INTEGER NOT NULL,
    "name" TEXT NOT NULL,
    "description" TEXT NOT NULL,
    "price" REAL NOT NULL,
    PRIMARY KEY ("id"),
    UNIQUE("product_id", "ticket_id")
);